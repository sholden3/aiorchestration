{
  "extends": "./app.config.json",
  "app": {
    "environment": "development"
  },
  "backend": {
    "host": "127.0.0.1",
    "port": 8000,
    "database": {
      "echo": true
    }
  },
  "frontend": {
    "electron": {
      "dev_tools": true
    }
  },
  "logging": {
    "level": "DEBUG",
    "console": {
      "colorize": true
    }
  },
  "features": {
    "ai_integration": {
      "enabled": false,
      "provider": "mock"
    }
  }
}